{
    "manifest_version": 3,
    "name": "Météo",
    "description": "Base Level Extension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon" : "./icons_128.png"
    },
    "icons" : {
      "16" : "./icons_16.png",
      "48" : "./icons_64.png",
      "128" : "./icons_128.png"
      
    },
    "permissions" : ["activeTab","geolocation"]
  }
  